# Third-Party Integrations Environment Variables
# Add these to your .env.local file

# Google Calendar Integration
GOOGLE_CALENDAR_CLIENT_ID=your_google_client_id_here
GOOGLE_CALENDAR_CLIENT_SECRET=your_google_client_secret_here

# Microsoft Outlook Calendar Integration (optional)
MICROSOFT_CALENDAR_CLIENT_ID=your_microsoft_client_id_here
MICROSOFT_CALENDAR_CLIENT_SECRET=your_microsoft_client_secret_here

# OpenTable Integration (if using their API)
OPENTABLE_API_KEY=your_opentable_api_key_here
OPENTABLE_API_SECRET=your_opentable_api_secret_here

# Resy Integration (if using their API)
RESY_API_KEY=your_resy_api_key_here

# DoorDash Integration (if using their API)
DOORDASH_API_KEY=your_doordash_api_key_here

# Uber Eats Integration (if using their API)
UBEREATS_API_KEY=your_ubereats_api_key_here

# Grubhub Integration (if using their API)
GRUBHUB_API_KEY=your_grubhub_api_key_here

# Social Media Integration Keys (if needed for enhanced features)
FACEBOOK_APP_ID=your_facebook_app_id_here
TWITTER_API_KEY=your_twitter_api_key_here
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here

# App Configuration
NEXT_PUBLIC_APP_URL=https://yumzoom.com
INTEGRATION_WEBHOOK_SECRET=your_webhook_secret_here
